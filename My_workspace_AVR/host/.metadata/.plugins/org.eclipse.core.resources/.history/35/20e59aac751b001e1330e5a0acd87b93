#include <stdio.h>
#include <stdint.h>

int main(void){

	int32_t num;

	printf("Enter number: %d",num);
	scanf("%d",&num);

	printf("\n4th: %d",(num | b(00001000)));
	printf("\n7th: %d",(num | b(01000000)));

	/* wait for the character */
	printf("\nPress enter to end application...");
	while(getchar() != '\n');

	getchar();
	return 0;
}
